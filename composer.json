{
    "name": "klimick/decode",
    "description": "Decoding untrusted data in typesafe way",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Klimenko Andrew (klimick)",
            "email": "klimichkartorgnusov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Klimick\\Decode\\": ["./src"],
            "Klimick\\PsalmDecode\\": ["./psalm"]
        },
        "files": [
            "decoders.php",
            "constraints.php",
            "src/HighOrder/Brand/OptionalBrand.php",
            "src/HighOrder/Brand/ConstrainedBrand.php",
            "src/HighOrder/Brand/DefaultBrand.php",
            "src/HighOrder/Brand/FromBrand.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Klimick\\Decode\\Test\\": ["./test"],
            "Psalm\\Mixins\\": ["./dev"]
        },
        "files": [
            "test/Helper/Eris.php"
        ]
    },
    "require": {
        "php": "^8.0",
        "whsv26/functional": "^4.10"
    },
    "require-dev": {
        "vimeo/psalm": "^4.15",
        "klimick/psalm-show-type": "^1.0",
        "psalm/plugin-phpunit": "^0.15.1",
        "giorgiosironi/eris": "^0.12.0",
        "phpunit/phpunit": "^9.5",
        "php-coveralls/php-coveralls": "^2.4",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "klimick/psalm-test": "^1.2"
    }
}
